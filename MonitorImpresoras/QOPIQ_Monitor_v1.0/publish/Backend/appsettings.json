{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=QOPIQ;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JwtSettings": {
    "SecretKey": "your-very-secure-jwt-key-here-that-is-at-least-256-bits-long-for-security",
    "Issuer": "QOPIQ.API",
    "Audience": "QOPIQ.Client",
    "ExpirationInMinutes": 60
  },
  "Snmp": {
    "Community": "public",
    "Timeout": 3000,
    "Retries": 2
  }
}
